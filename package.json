{
  "name": "lassus-tricinium-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run server",
    "prod": "npm run build && npm run server:prod",
    "build": "vite build && vite build --ssr",
    "server": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "preview": "vite preview --port 5050",
    "generate": "rm -rf dist/ && vite build && vite build --ssr && vite-plugin-ssr prerender",
    "deploy": "npm run generate && rsync -azP dist/client/ wolfgang:lassus.wolfgangdrescher.ch --delete",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.2.31",
    "compression": "^1.7.4",
    "diff": "^5.0.0",
    "diff2html": "^3.4.16",
    "express": "^4.17.3",
    "lassus-geistliche-psalmen": "file:../lassus-geistliche-psalmen",
    "vite-plugin-ssr": "^0.3.59",
    "vue": "^3.2.29",
    "vue-verovio-canvas": "file:../../vue-verovio-canvas"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "vite": "^2.7.13"
  }
}
